#26.12.2022 23:14 (356)

alph = '0123456789abcde'
alph1 = '0123456789abcdefg'
Ymin = 17
for x in alph:
    for y in alph1:
        a1 = "123" + x + "5"
        a2 = "67" + y + "9"
        summ = int(a1,15) + int(a2,17)
        if summ % 131 == 0 and int(y,17)<Ymin:
            maxx = summ // 131
            Ymin = int(y,17)
print(maxx)

#26.12.2022 23:22 (357)     

alph = "0123456789abc"
alph1 = "123456789" 
for i in range(12):
    alph1 +=chr(ord("a")+i)

XYmin = 34
for x in alph1:
    for y in alph:
        a1 = x +"23" + x + "5"
        a2 = "67" + y + "9" + y
        minus = int(a1,22) - int(a2,13)
        if  minus % 57 == 0 and (int(x,22) + int(y,13))<XYmin :
            murp = minus // 57 
            XYmin = int(x,22) + int(y,13)
print(murp)      

