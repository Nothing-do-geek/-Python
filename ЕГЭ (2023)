#29.02.2024 20:46 (11 (Задача 15))
def f(x):
     return (x & A != 0)and(x & 48 == 0)and(x & 27 == 0)

for A in range(1,100):
     if any(f(x)!=0 for x in range(1,100)):
          print(A)

