for x in alph:
    a1 = "66" + x + "63"
    a2 = "5" + x + "810" #f = int(f'66{x}63',17) - int(f'5{x}810',17)
    summ = int(a1,17) - int(a2,17)
    if summ % 11 == 0:
        print(summ // 11)
        break

