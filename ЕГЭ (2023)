#29.02.2024 20:19 (2 (Задача 5))
for n in range(1,1000):
     b = bin(n)[2:]
     if n % 2 == 0:
          b = b + '0'
     else:
          b = '1' + b
     r = int(b,2)
     if r < 95:
          print(r)
          break
#29.02.2024 20:48 (5 (Задача 8))
s = 'СУМКА'
k = 0

for a in s:
     for b in s:
          for c in s:
               for d in s:
                    w = a+b+c+d
                    if w.count('У') <= 1:
                         k += 1
print(k)
